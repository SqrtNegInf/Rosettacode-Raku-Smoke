#!/usr/bin/env perl6
#u# http://rosettacode.org/wiki/Fibonacci_word/fractal
#c# 2016-03-07 <RC
#m# MOAR: OK
#j#  JVM: OK
#n# see also 'U/Ulam_spiral_for_primes' which has modified Braille-graphics code

my @res;

constant @fib-word = '1', '0', { $^b ~ $^a } ... *;

my ($m, $scale) = 17, 3;

(my %world){0}{0} = 1;
my $loc = 0+0i;
my $dir = i;
my $n = 1;

for @fib-word[$m].comb {
    when '0' {
        step;
        if $n %% 2 { turn-left }
        else { turn-right; }
    }
    $n++;
}

braille-graphics %world;

sub step {
    for ^$scale {
        $loc += $dir;
        %world{$loc.im}{$loc.re} = 1;
    }
}

sub turn-left  { $dir ×=  i; }
sub turn-right { $dir ×= -i; }

sub braille-graphics (%a) {
    my ($ylo, $yhi, $xlo, $xhi);
    for %a.keys -> $y {
        $ylo min= +$y; $yhi max= +$y;
        for %a{$y}.keys -> $x {
            $xlo min= +$x; $xhi max= +$x;
        }
    }

    for $ylo, $ylo + 4 ...^ * > $yhi -> \y {                       # } }  vim kludge
        my $line;
        for $xlo, $xlo + 2 ...^ * > $xhi -> \x {
            my $cell = 0x2800;
            $cell +=   1 if %a{y + 0}{x + 0};
            $cell +=   2 if %a{y + 1}{x + 0};
            $cell +=   4 if %a{y + 2}{x + 0};
            $cell +=   8 if %a{y + 0}{x + 1};
            $cell +=  16 if %a{y + 1}{x + 1};
            $cell +=  32 if %a{y + 2}{x + 1};
            $cell +=  64 if %a{y + 3}{x + 0};
            $cell += 128 if %a{y + 3}{x + 1};
            $line ~= chr($cell);
        }
        @res.push: $line;
    }
}

#.say for @res;

my $ref = qq:to/END/;
