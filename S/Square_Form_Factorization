my @res;

my @multiplier = ( 1, 3, 5, 7, 11, 3Ã—5, 3Ã—7, 3Ã—11, 5Ã—7, 5Ã—11, 7Ã—11, 3Ã—5Ã—7, 3Ã—5Ã—11, 3Ã—7Ã—11, 5Ã—7Ã—11, 3Ã—5Ã—7Ã—11 );

sub circumfix:<âŒŠ âŒ‹>{ $^n.floor }; sub prefix:<âˆš>{ $^n.sqrt }; # just for fun

sub SQUFOF ( \ð‘ ) {  

   return  1 if ð‘.is-prime;     # if n is prime             return  1
   return âˆšð‘ if âˆšð‘ == Int(âˆšð‘);  # if n is a perfect square  return âˆšð‘

   for @multiplier -> \ð‘˜ { 
      my \Pâ‚’ = $ = âŒŠ âˆš(ð‘˜Ã—ð‘) âŒ‹;          # P[0]=floor(âˆšN)
      my \Qâ‚’ = $ = 1 ;                  # Q[0]=1
      my \Q = $ =  ð‘˜Ã—ð‘ - Pâ‚’Â²;           # Q[1]=N-P[0]^2 & Q[i]
      my \Pâ‚šáµ£â‚‘áµ¥ = $ = Pâ‚’;               # P[i-1] = P[0]
      my \Qâ‚šáµ£â‚‘áµ¥ = $ = Qâ‚’;               # Q[i-1] = Q[0] 
      my \P = $ = 0;                    # P[i]
      my \Qâ‚™â‚‘â‚“â‚œ = $ = 0;                # P[i+1]
      my \b = $ = 0;                    # b[i]
                                        # i = 1
      repeat until âˆšQ == Int(âˆšQ) {      # until Q[i] is a perfect square
         b = âŒŠâŒŠ âˆš(ð‘˜Ã—ð‘) + Pâ‚šáµ£â‚‘áµ¥ âŒ‹ / Q âŒ‹;    # floor(floor(âˆšN+P[i-1])/Q[i])
         P = bÃ—Q - Pâ‚šáµ£â‚‘áµ¥;                  # P[i]=bÃ—Q[i]-P[i-1] 
         Qâ‚™â‚‘â‚“â‚œ = Qâ‚šáµ£â‚‘áµ¥ + bÃ—(Pâ‚šáµ£â‚‘áµ¥ - P);    # Q[i+1]=Q[i-1]+b(P[i-1]-P[i]) 
         ( Qâ‚šáµ£â‚‘áµ¥,  Q, Pâ‚šáµ£â‚‘áµ¥ ) = Q, Qâ‚™â‚‘â‚“â‚œ, P;  # i++
      } 

      b     = âŒŠ âŒŠ âˆš(ð‘˜Ã—ð‘)+P âŒ‹ / Q âŒ‹;     # b=floor((floor(âˆšN)+P[i])/Q[0])
      Pâ‚šáµ£â‚‘áµ¥ = bÃ—Qâ‚’ - P;                 # P[i-1]=bÃ—Q[0]-P[i]
      Q     = ( ð‘˜Ã—ð‘ - Pâ‚šáµ£â‚‘áµ¥Â² )/Qâ‚’;      # Q[1]=(N-P[0]^2)/Q[0] & Q[i]
      Qâ‚šáµ£â‚‘áµ¥ = Qâ‚’;                       # Q[i-1] = Q[0]
                                        # i = 1
      loop {                            # repeat
         b  = âŒŠ âŒŠ âˆš(ð‘˜Ã—ð‘)+Pâ‚šáµ£â‚‘áµ¥ âŒ‹ / Q âŒ‹;    # b=floor(floor(âˆšN)+P[i-1])/Q[i])
         P  = bÃ—Q - Pâ‚šáµ£â‚‘áµ¥;                 # P[i]=bÃ—Q[i]-P[i-1]
         Qâ‚™â‚‘â‚“â‚œ = Qâ‚šáµ£â‚‘áµ¥ + bÃ—(Pâ‚šáµ£â‚‘áµ¥ - P);    # Q[i+1]=Q[i-1]+b(P[i-1]-P[i])
         last if (P == Pâ‚šáµ£â‚‘áµ¥);          # until P[i+1]=P[i]
         ( Qâ‚šáµ£â‚‘áµ¥,  Q, Pâ‚šáµ£â‚‘áµ¥ ) = Q, Qâ‚™â‚‘â‚“â‚œ, P; # i++ 
      }  
      given ð‘ gcd P { return $_ if $_ != 1|ð‘ }    
   }  # gcd(N,P[i]) (if != 1 or N) is a factor of N, otherwise try next k
   return 0 # give up
}

race for ( 
   11111, # wikipedia.org/wiki/Shanks%27s_square_forms_factorization#Example
   4558849, # example from talk page
   # all of the rest are taken from the FreeBASIC entry
   2501,12851,13289,75301,120787,967009,997417,7091569,13290059,
   42854447,223553581,2027651281,11111111111,100895598169,1002742628021,
   # time hoarders 
#  60012462237239, # = 6862753 Ã— 8744663                     15s 
#  287129523414791, # = 6059887 Ã— 47381993                   80s
#  11111111111111111, # = 2071723 Ã— 5363222357                2m
#  384307168202281507, # = 415718707 Ã— 924440401              5m
#  1000000000000000127, # = 111756107 Ã— 8948056861           12m
#  9007199254740931, # = 10624181 Ã— 847801751                17m
#  922337203685477563, # = 110075821 Ã— 8379108103            41m
#  314159265358979323, # = 317213509 Ã— 990371647             61m
#  1152921505680588799, # = 139001459 Ã— 8294312261           93m
#  658812288346769681, # = 62222119 Ã— 10588072199           112m
#  419244183493398773, # = 48009977 Ã— 8732438749            135m
#  1537228672809128917, # = 26675843 Ã— 57626245319          254m
   # don't know how to handle this one
   # for 1e-323, 1e-324 { my $*TOLERANCE = $_ ; 
   #     say 4611686018427387877.sqrt â‰… 4611686018427387877.sqrt.Int }
   # skip the perfect square check and start k with 3 to get the following 
   # 4611686018427387877, # = 343242169 Ã— 13435662733       217m
) -> \data {
   given data.&SQUFOF {
      when 0  { @res.push: 'The number ' ~ data ~ ' is not factored.' }
      when 1  { @res.push: 'The number ' ~ data ~ ' is a prime.' }
      default { @res.push: data ~ " = $_ Ã— " ~ data div $_.Int }
   }
}

.say for @res;

