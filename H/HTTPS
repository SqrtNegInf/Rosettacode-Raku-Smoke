use WWW;

my $result1 = get 'https://google.com/';

use HTTP::UserAgent;
my $result2 = HTTP::UserAgent.new.get('https://google.com/').content;

use Test;
ok $result1 ~~ /'Feeling Lucky'/;
ok $result2 ~~ /'Feeling Lucky'/;
