#!/usr/bin/env perl6
#u# http://rosettacode.org/wiki/99_Bottles_of_Beer
#c# 2015-11-01 <RC
#m# MOAR: OK
#j#  JVM: OK

my $b = 99;

my @res1;

repeat while --$b {
    @res1.push: "{b $b} on the wall";
    @res1.push: "{b $b}";
    @res1.push: "Take one down, pass it around";
    @res1.push: "{b $b-1} on the wall";
    @res1.push: "";
}

sub b($b) {
    "$b bottle{'s' if $b != 1} of beer";
}

.say for @res1;

my $ref = qq:to/END/;
