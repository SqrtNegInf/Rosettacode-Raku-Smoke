912985153
END

use Test;
is $result, chomp $ref;

=finish 

( 10**($lo-1) .. 10**$hi - 1 ).race.map: -> $n {
}

#.say for @n.grep(so *).sort;
say my $result = @n.grep(so *).sort.join("\n");
exit;
