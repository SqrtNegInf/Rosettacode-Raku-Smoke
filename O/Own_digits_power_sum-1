#!/usr/bin/env perl6
#u# http://rosettacode.org/wiki/Own_digits_power_sum
#t# inprogress
#c# 2021-10-xx NOT ON RC
#m# MOAR: OK
#j# JVM:  OK
#n# combinatorics

for 3..4 -> $d {
    for [X](^10 xx $d).unique(as => *.sort.cache, with => &[eqv]) -> @digits {
        .say if $d == .comb.elems and @digits.join == .comb.sort.join given sum @digits X** $d;
    }
}

exit;
my $result;

my $ref = q:to/END/;
153
