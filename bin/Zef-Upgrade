#!/bin/sh
## update ecosystem modules and report

source $HOME/.profile
cd $HOME/.rakubrew
#bin/rakubrew mode shim

#bin/rakubrew build zef
~/.rakubrew/versions/moar-blead/install/share/perl6/site/bin/zef upgrade > out.zef 2> err.zef

echo ' '
echo 'out.zef'
/usr/local/bin/pcregrep 'FAIL|OK' out.zef | sort
echo ' '
echo 'err.zef'
/usr/local/bin/pcregrep -v mirror: err.zef | sort

#bin/rakubrew mode env
