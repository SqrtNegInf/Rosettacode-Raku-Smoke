#!/usr/bin/env raku
#u# http://rosettacode.org/wiki/Generate_random_numbers_without_repeating_a_value
#c# 2021-08-28 <RC
#m# MOAR: OK
#j# JVM:  BROKEN
#n# illustrate use of enhanced 'pick'

use v6.e.PREVIEW; 

srand 123456;

my @res.push: $_ for (1..20).pick(**).[^100].batch(20);

.say for @res;

my $ref = q:to/END/;
18 14 7 6 12 11 3 17 20 1 8 10 15 19 2 4 5 13 9 16
18 3 12 19 16 11 14 7 20 17 15 6 1 5 8 2 9 4 10 13
4 12 15 10 19 9 8 17 18 5 16 3 20 2 7 1 13 6 14 11
15 16 2 20 18 1 13 10 17 11 8 3 19 6 5 12 7 14 4 9
4 6 7 2 11 13 15 9 12 8 1 3 16 10 17 14 5 19 18 20
END

use Test;
is @res.join("\n"), chomp $ref;
